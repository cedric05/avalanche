{
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "routePrefix": "customPrefix",
        "direction": "in",
        "name": "req",
        "route": "{*route}",
        "methods": [
          "get",
          "post",
          "delete",
          "put",
          "patch",
          "head",
          "options"
        ]
      },
      {
        "type": "http",
        "direction": "out",
        "name": "res"
      }
    ]
  }